<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.zero.customer.mapper.slave.SelectServerMapper">    <resultMap id="serverData" type="com.zero.customer.db.Server">        <result column="uuid" property="uuid" jdbcType="INTEGER"/>        <result column="server_name" property="serverName" jdbcType="VARCHAR"/>        <result column="serverImple_name" property="serverImplName" jdbcType="VARCHAR"/>        <result column="method_name" property="methodName" jdbcType="VARCHAR"/>        <result column="create_user" property="createName" jdbcType="VARCHAR"/>        <result column="update_user" property="updateName" jdbcType="VARCHAR"/>    </resultMap>    <sql id="dynamicWhere">        <where>            1=1            <if test="@Ognl@isNotEmpty(uuid)">and uuid = #{uuid}</if>            <if test="@Ognl@isNotEmpty(userName)">and user_name like '%${userName}%'</if>            <if test="@Ognl@isNotEmpty(userPhone)">and user_phone like '%${userPhone}%'</if>            <if test="@Ognl@isNotEmpty(userAccount)">and user_account like '%${userAccount}%'</if>            <if test="@Ognl@isNotEmpty(userSex)">and user_sex like '%${userSex}%'</if>            <if test="@Ognl@isNotEmpty(userAge)">and user_age = #{userAge}</if>            <if test="@Ognl@isNotEmpty(userState)">and user_state = #{userState}</if>            <if test="@Ognl@isNotEmpty(createTime)"> AND create_time &gt;= #{createTime,jdbcType=TIMESTAMP} </if>            <if test="@Ognl@isNotEmpty(updateTime)"> AND update_time &lt;= #{updateTime,jdbcType=TIMESTAMP} </if>        </where>    </sql>    <select id="getServer" resultMap="serverData" parameterType="integer" >        select *from server where uuid = #{uuid}    </select></mapper>