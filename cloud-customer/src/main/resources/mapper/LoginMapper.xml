<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.zero.customer.mapper.master.SelectMapper">    <resultMap id="userData" type="com.zero.customer.db.User">        <result column="uuid" property="uuid" jdbcType="INTEGER"/>        <result column="user_name" property="userName" jdbcType="VARCHAR"/>        <result column="user_password" property="userPassword" jdbcType="VARCHAR"/>        <result column="user_phone" property="userPhone" jdbcType="VARCHAR"/>        <result column="user_account" property="userAccount" jdbcType="VARCHAR"/>        <result column="user_email" property="userEmail" jdbcType="VARCHAR"></result>        <result column="user_sex" property="userSex" jdbcType="VARCHAR"/>        <result column="user_age" property="userAge" jdbcType="INTEGER"/>        <result column="user_state" property="userState" jdbcType="INTEGER"/>        <result column="user_image" property="userImage" jdbcType="BINARY"/>        <result column="user_creadits" property="userCreadits" jdbcType="INTEGER" />        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>    </resultMap>    <sql id="dynamicWhere">        <where>            1=1            <if test="@Ognl@isNotEmpty(uuid)">and uuid = #{uuid}</if>            <if test="@Ognl@isNotEmpty(userName)">and user_name like '%${userName}%'</if>            <if test="@Ognl@isNotEmpty(userPhone)">and user_phone like '%${userPhone}%'</if>            <if test="@Ognl@isNotEmpty(userAccount)">and user_account like '%${userAccount}%'</if>            <if test="@Ognl@isNotEmpty(userSex)">and user_sex like '%${userSex}%'</if>            <if test="@Ognl@isNotEmpty(userAge)">and user_age = #{userAge}</if>            <if test="@Ognl@isNotEmpty(userState)">and user_state = #{userState}</if>            <if test="@Ognl@isNotEmpty(createTime)"> AND create_time &gt;= #{createTime,jdbcType=TIMESTAMP} </if>            <if test="@Ognl@isNotEmpty(updateTime)"> AND update_time &lt;= #{updateTime,jdbcType=TIMESTAMP} </if>        </where>    </sql>    <update id="updateUser" parameterType="com.zero.customer.db.User">        update user set        uuid=#{uuid}        <if test="userName != null and userName !='' " >            ,user_name=#{userName}        </if>        <if test="userPassword != null and userPassword !='' " >            ,user_password=#{userPassword}        </if>        <if test="userPhone != null and userPhone !='' " >            ,user_phone=#{userPhone}        </if>        <if test="userAccount != null and userAccount !='' " >            ,user_account=#{userAccount}        </if>        <if test="userSex != null and userSex !='' " >            ,user_sex=#{userSex}        </if>        <if test="userAge != null and userAge !='' " >            ,user_age=#{userAge}        </if>        <if test="userState != null and userState !='' " >            ,user_state = #{userState}        </if>        <if test="userImage != null and userImage !='' " >            ,user_image=#{userImage}        </if>        <if test="userCreadits != null and userCreadits !='' " >            ,user_creadits=#{userCreadits}        </if>        where uuid=#{uuid};    </update>    <select id="getUserCredits" resultMap="userData" parameterType="integer" >        select *from user where uuid = #{uuid}    </select></mapper>